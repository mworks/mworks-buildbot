<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en-us">
  <title type="text">{{ config.title }}</title>
  <updated>{{ config.updated.isoformat() }}</updated>
  <id>{{ config.base_url }}</id>
  <link rel="alternate" type="text/html" href="{{ config.base_url }}"/>
  <link rel="self" type="application/atom+xml" href="{{ config.base_url }}atom_feed/"/>
  <author>
    <name>{{ config.author }}</name>
  </author>
  {% for info in builds %}
  <entry>
    <title type="text">{{ info.title }}</title>
    <link rel="alternate" type="text/html" href="{{ info.url }}"/>
    <id>{{ info.url }}</id>
    <updated>{{ info.complete_at.isoformat() }}</updated>
    <content type="text">{{ info.state_string }}</content>
  </entry>
  {% endfor %}
</feed>
